<h1>Edit Contacts</h1>

<%= render "form", contact: @contact %>

<br>

<div>
  <%= link_to "Show this contact", @contact, class: "btn btn-dark"  %>
  <%= link_to 'Delete', @contact, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-dark" %>
  <%= link_to "Back to contacts", contacts_path, class: "btn btn-dark"  %>

</div>
